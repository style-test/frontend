{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "trailing-comma": [
      true,
      {
        "esSpecCompliant": true
      }
    ],
    "no-submodule-imports": [true, "redux-saga"]
  },
  "rulesDirectory": []
}
